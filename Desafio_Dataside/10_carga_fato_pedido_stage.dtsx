                <property
                  dataType="System.String"
                  description="O comando SQL a ser executado."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT 
                         ft_pedido.order_id AS id_pedido, ft_pedido.customer_id AS fk_cliente, itens_p.seller_id AS fk_vendedor, itens_p.product_id AS fk_produto, ft_pedido.order_status AS status_do_pedido, 
                         CAST(ft_pedido.order_purchase_timestamp AS DATE) AS dt_compra, CAST(ft_pedido.order_approved_at AS DATE) AS dt_aprovacao, CAST(ft_pedido.order_purchase_timestamp AS DATE) AS dt_estimativa_entrega, 
                         CAST(itens_p.shipping_limit_date AS DATE) AS dt_limite_envio, CAST(ft_pedido.order_delivered_carrier_date AS DATE) AS dt_envio, CAST(ft_pedido.order_delivered_customer_date AS DATE) AS dt_entrega, 
                         pgto_ped.payment_value AS valor_total_pago, valor_frete.sum_freight_value AS valor_frete, pgto_ped.payment_installments AS qtde_parcelas, valor_frete.qtde AS qtde_itens_pedido, volume.volume_cm3, 
                         volume.peso_total_g, CAST(concat_ws('_', pgto.order_id, pgto.payment_type, pgto_ped.payment_value) AS varchar(255)) AS chave_pgto
FROM            [order] AS ft_pedido LEFT OUTER JOIN
                         order_items AS itens_p ON ft_pedido.order_id = itens_p.order_id LEFT OUTER JOIN
                         order_payments AS pgto_ped ON ft_pedido.order_id = pgto_ped.order_id LEFT OUTER JOIN
                             (SELECT        order_id, SUM(freight_value) AS sum_freight_value, MAX(order_item_id) AS qtde
                               FROM            order_items
                               GROUP BY order_id) AS valor_frete ON ft_pedido.order_id = valor_frete.order_id LEFT OUTER JOIN
                             (SELECT        itens_p.order_id, SUM(CAST(prod.product_length_cm AS INT) * CAST(prod.product_height_cm AS INT) * CAST(prod.product_width_cm AS INT)) AS volume_cm3, SUM(prod.product_weight_g) AS peso_total_g
                               FROM            products AS prod INNER JOIN
                                                         order_items AS itens_p ON prod.product_id = itens_p.product_id
                               GROUP BY itens_p.order_id) AS volume ON ft_pedido.order_id = volume.order_id LEFT OUTER JOIN
                         order_payments AS pgto ON pgto.order_id = itens_p.order_id</property>
